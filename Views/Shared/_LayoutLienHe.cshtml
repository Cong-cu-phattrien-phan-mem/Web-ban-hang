</head>
<body>
	<!--header-->
	<div class="header">
		<div class="header-top">
			<div class="container">
				<div class="search">
					<form>
						<input type="text" value="Search " onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Search';}">
						<input type="submit" value="Go">
					</form>
				</div>
				<div class="header-left">
					<ul>
						<li><a href="@Url.Action("Dangnhap","TaiKhoan")">Đăng nhập</a></li>
						<li><a href="@Url.Action("DangKy","TaiKhoan")">Đăng ký</a></li>
						<li>@Html.Action("GiohangPartial", "Giohang")</li>


					</ul>
					<div class="clearfix"> </div>
				</div>
				<div class="clearfix"> </div>
			</div>
		</div>
		<div class="container">
			<div class="head-top">
				<div class="logo">
                    <a href="@Url.Action("Index","TrangChu")"><img src="~/images/free_horizontal_on_white_by_logaster.png" alt=""></a>
				</div>
				<div class=" h_menu4">
					<ul class="memenu skyblue">
						<li class="active grid"><a class="color8" href="@Url.Action("Index","TrangChu")">Home</a></li>
						<li>
							<a class="color1" href="#">Danh mục</a>
							<div class="mepanel">
								<div class="row">
									<div class="col1">
										<div class="h_nav">
											<h4>Sản Phẩm:</h4>
											<ul>
												<li>
													@Html.Action("Danhmuc", "TrangSanPham")
												</li>
											</ul>
										</div>
									</div>
									<div class="col1">
										<div class="h_nav">
											<h4>Gian Hàng:</h4>
											<ul>
												<li>
													@Html.Action("Loai", "TrangSanPham")
												</li>
											</ul>
										</div>
									</div>
								</div>
							</div>
						</li>

                        <li><a class="color2" href=""@Url.Action("Index", "GioiThieu")"">Giới Thiệu</a></li>

						<li><a class="color4" href="@Url.Action("Index", "Blog")">Blog</a></li>
						<li><a class="color6" href="@Url.Action("LienHe", "TrangChu")">Liên Hệ</a></li>
					</ul>
				</div>

				<div class="clearfix"> </div>
			</div>
		</div>

	</div>


	<!--content-->
	<div class="contact">

		<div class="container">
			<h1>Liên Hệ</h1>
			<div class="contact-form">
				@RenderBody()
				<div class="col-md-8 contact-grid">
					<form>
						<input type="text" value="Name" onfocus="this.value='';" onblur="if (this.value == '') {this.value ='Name';}">

						<input type="text" value="Email" onfocus="this.value='';" onblur="if (this.value == '') {this.value ='Email';}">
						<input type="text" value="Subject" onfocus="this.value='';" onblur="if (this.value == '') {this.value ='Subject';}">

						<textarea cols="77" rows="6" value=" " onfocus="this.value='';" onblur="if (this.value == '') {this.value = 'Message';}">Message</textarea>
						<div class="send">
							<input type="submit" value="Gửi">
						</div>
					</form>
				</div>
				<div class="col-md-4 contact-in">

					<div class="address-more">
						<h4>Địa Chỉ</h4>
						<p>Tel: 0393592609</p>
						<p>Địa Chỉ:480/53 Bình Quới, Phường 28, Bình Thạnh, Hồ Chí Minh</p>
						<p>Email:<a href="#"> tampham.1999.cancer@gmail.com</a></p>
					</div>

				</div>
				<div class="clearfix"> </div>
			</div>
